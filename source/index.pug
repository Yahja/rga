doctype html
html(lang='en')
  head
    // Required meta tags
    meta(charset='utf-8')
    meta(name='viewport', content='width=device-width, initial-scale=1, shrink-to-fit=no')
    // Bootstrap CSS
    link(rel='stylesheet', href='css/styles.css')
    title Basic Template
  body
    .container-fuid
      .row.m-0
        .col-3.col-sm-2.col-md-2.col-lg-1.offset-lg-1.position-absolute.logo-top
            img.img-fluid(src='images/logo-falabella.jpg')
        .col.p-0.img-main
            img.img-fluid(src='images/HH1-color-040219-JP-2x.jpg')
      .row.justify-content-center.m-0#colors
        .col-6.col-sm-6.col-md-3 
            img.img-fluid.img-colors(src='images/color-010319-naranjo.jpg')
        .col-6.col-sm-6.col-md-3
            img.img-fluid.img-colors(src='images/color-010319-morado.jpg')
        .col-6.col-sm-6.col-md-3
            img.img-fluid.img-colors(src='images/color-010319-rosado.jpg')
        .col-6.col-sm-6.col-md-3
            img.img-fluid.img-colors(src='images/color-010319-amarillo.jpg')
      ul.row.ml-0.mr-0.p-0.menu-colores
        li.col.naranjo
          a(onclick='functionClick()') Naranjo
        li.col.morado
          a(onclick='functionClick()') Morado
        li.col.rosado
          a(onclick='functionClick()') Rosado
        li.col.amarillo
          a(onclick='functionClick()') Amarillo
    script(src='https://cdnjs.cloudflare.com/ajax/libs/gsap/2.1.2/TweenMax.min.js')
    script(src='js/app.min.js')